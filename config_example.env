# MAIC JSON Service with EyeLink Integration 配置示例

# 基础服务配置
MAIC_PORT=8123

# EyeLink 眼动仪配置
EYELINK_HOST_IP=100.1.1.1
EYELINK_DUMMY_MODE=false
EYELINK_SCREEN_WIDTH=1920
EYELINK_SCREEN_HEIGHT=1080
EYELINK_EDF_FILENAME=experiment.edf

# 启动时自动连接 EyeLink（默认: true）
EYELINK_AUTO_CONNECT=true

# 连接后自动开始记录（默认: true）
EYELINK_AUTO_RECORD=true

# 日志配置
LOG_LEVEL=DEBUGs
